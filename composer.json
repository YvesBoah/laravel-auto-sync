{
    "name": "mohammed-zaki/laravel-auto-sync",
    "type": "library",
    "description": "Automatic Synchronize MySql database changes on remote locations for laravel applications (Multi slave servers to one Master server replication).",
    "keywords": [
        "laravel",
        "auto-sync",
        "synchronize",
        "MySql",
        "database",
        "slave-to-master-replication",
        "replication",
        "sql-logger"
    ],
    "license": "MIT",
    "homepage": "https://github.com/mohammedzaki/laravel-auto-sync",
    "support": {
        "issues": "https://github.com/mohammedzaki/laravel-auto-sync/issues",
        "source": "https://github.com/mohammedzaki/laravel-auto-sync"
    },
    "authors": [
        {
            "name": "Mohammed Zaki",
            "email": "mohammedzaki.dev@gamil.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/console": "5.5.*",
        "illuminate/filesystem": "5.5.*",
        "illuminate/support": "5.5.*",
        "illuminate/database": "5.5.*",
        "monolog/monolog": "~1.12"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "AutoSync\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "AutoSync\\Providers\\SqlLogServiceProvider"
            ]
        }
    }
}
